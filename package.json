{
  "name": "oicd",
  "version": "1.0.0",
  "description": "OICD client",
  "author": "Thomas Maier",
  "build": {
    "appId": "oicd",
    "productName": "OICD"
  },
  "main": "./public/electron.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "electron-is-dev": "2.0.0",
    "node-sass": "7.0.1",
    "oidc-react": "^1.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "concurrently": "7.2.2",
    "cross-env": "7.0.3",
    "electron": "19.0.8",
    "electron-builder": "23.1.0",
    "react-scripts": "5.0.1",
    "typescript": "4.7.4",
    "wait-on": "6.0.1"
  },
  "scripts": {
    "dev": "concurrently \"cross-env BROWSER=none yarn react:start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "electron-builder --publish=never",
    "electron:package": "yarn react:build && yarn electron:build",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "release": "yarn react:build && electron-builder --publish=never"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version"
    ]
  }
}
